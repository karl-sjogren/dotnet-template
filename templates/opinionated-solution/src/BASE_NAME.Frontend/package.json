{
  "name": "BASE_NAME.frontend",
  "packageManager": "yarn@4.0.2",
  "version": "0.0.0",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint:css": "stylelint (styles|static)/**/*.{scss,css}",
    "lint:js": "eslint scripts/**/*.{js,mjs}",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --coverage",
    "test:coverage-report": "vitest run --coverage && npx --yes open-cli ./coverage/index.html",
    "test:ci": "vitest run --reporter=junit --coverage --outputFile=test-results/junit.xml"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.5.3",
    "@stylistic/stylelint-plugin": "^2.0.0",
    "@testing-library/dom": "^9.3.4",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.16",
    "browserslist": "^4.22.2",
    "cssnano": "^6.0.3",
    "esbuild": "^0.19.11",
    "esbuild-plugin-browserslist": "^0.10.0",
    "eslint": "^8.56.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-vitest": "^0.3.2",
    "jsdom-testing-mocks": "^1.11.0",
    "postcss": "^8.4.33",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "vite": "^5.0.11",
    "vite-plugin-mkcert": "^1.17.2",
    "vitest": "^1.2.0"
  }
}
